{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Production-ready boilerplate for building universal web apps with React and Redux",
  "engines": {
    "node": "7.2.0"
  },
  "scripts": {
    "start": "npm run serve",
    "serve": "node -r ./server/environment ./server",
    "build": "node ./tasks/build",
    "test": "jest --coverage",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "precommit": "lint-staged",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "MIT",
  "homepage": "https://sixtyframes-react-boilerplate.herokuapp.com",
  "devDependencies": {
    "async": "^2.1.5",
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "coveralls": "^2.12.0",
    "css-loader": "^0.28.0",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.4.3",
    "eslint": "^3.19.0",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "fs-extra": "^3.0.0",
    "husky": "^0.13.3",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^0.7.1",
    "jest": "^19.0.2",
    "json-loader": "^0.5.4",
    "lint-staged": "^3.4.0",
    "postcss-loader": "^1.3.3",
    "prettier": "^1.2.2",
    "progress-bar-webpack-plugin": "^1.9.3",
    "react-hot-loader": "^3.0.0-beta.1",
    "react-test-renderer": "^15.4.2",
    "redux-mock-store": "^1.2.2",
    "require-from-string": "^1.1.0",
    "stats-webpack-plugin": "^0.5.0",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.2",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-hot-server-middleware": "^0.1.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "colors": "^1.1.2",
    "compression": "^1.6.1",
    "css-reset": "github:60frames/css-reset#1.0.0",
    "debug": "^2.6.3",
    "es6-error": "^4.0.2",
    "express": "^4.15.2",
    "isomorphic-fetch": "^2.2.1",
    "ms": "^1.0.0",
    "prop-types": "^15.5.8",
    "react": "^15.3.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.0.0",
    "react-loadable": "^3.3.1",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.8",
    "redux": "^3.4.0",
    "redux-logger": "^3.0.0",
    "redux-thunk": "^2.2.0",
    "serialize-javascript": "^1.3.0"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "^.+\\.css$": "<rootDir>/test/styleMock.js"
    },
    "testEnvironment": "node"
  },
  "browserslist": [
    "> 1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "npm test"
    ]
  }
}
